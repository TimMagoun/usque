@misc{sjoberg_lie_2020,
	title = {Lie {Algebraic} {Unscented} {Kalman} {Filter} for {Pose} {Estimation}},
	url = {http://arxiv.org/abs/2005.00385},
	abstract = {An unscented Kalman filter for matrix Lie groups is proposed where the time propagation of the state is formulated on the Lie algebra. This is done with the kinematic differential equation of the logarithm, where the inverse of the right Jacobian is used. The sigma points can then be expressed as logarithms in vector form, and time propagation of the sigma points and the computation of the mean and the covariance can be done on the Lie algebra. The resulting formulation is to a large extent based on logarithms in vector form, and is therefore closer to the UKF for systems in \${\textbackslash}mathbb\{R\}{\textasciicircum}n\$. This gives an elegant and well-structured formulation which provides additional insight into the problem, and which is computationally efficient. The proposed method is in particular formulated and investigated on the matrix Lie group \$SE(3)\$. A discussion on right and left Jacobians is included, and a novel closed form solution for the inverse of the right Jacobian on \$SE(3)\$ is derived, which gives a compact representation involving fewer matrix operations. The proposed method is validated in simulations.},
	urldate = {2023-05-01},
	publisher = {arXiv},
	author = {Sjøberg, Alexander Meyer and Egeland, Olav},
	month = apr,
	year = {2020},
	note = {arXiv:2005.00385 [cs, eess, math]},
	keywords = {Computer Science - Robotics, Electrical Engineering and Systems Science - Systems and Control, Mathematics - Optimization and Control},
	file = {arXiv Fulltext PDF:/home/tim/Zotero/storage/E56GD42F/Sjøberg and Egeland - 2020 - Lie Algebraic Unscented Kalman Filter for Pose Est.pdf:application/pdf;arXiv.org Snapshot:/home/tim/Zotero/storage/W3UD6YCN/2005.html:text/html},
}

@article{crassidis_unscented_nodate,
	title = {Unscented {Filtering} for {Spacecraft} {Attitude} {Estimation}},
	abstract = {A new spacecraft attitude estimation approach based on the Unscented Filter is derived. For nonlinear systems the Unscented Filter uses a carefully selected set of sample points to more accurately map the probability distribution than the linearization of the standard Extended Kalman Filter, leading to faster convergence from inaccurate initial conditions in attitude estimation problems. The ﬁlter formulation is based on standard attitude-vector measurements using a gyro-based model for attitude propagation. The global attitude parameterization is given by a quaternion, while a generalized three-dimensional attitude representation is used to deﬁne the local attitude error. A multiplicative quaternion-error approach is derived from the local attitude error, which guarantees that quaternion normalization is maintained in the ﬁlter. Simulation results indicate that the Unscented Filter is more robust than the Extended Kalman Filter under realistic initial attitude-error conditions.},
	language = {en},
	author = {Crassidis, John L and Markley, F Landis},
	file = {Crassidis and Markley - Unscented Filtering for Spacecraft Attitude Estima.pdf:/home/tim/Zotero/storage/AW8HNVBF/Crassidis and Markley - Unscented Filtering for Spacecraft Attitude Estima.pdf:application/pdf},
}

